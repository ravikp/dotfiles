alias ll='ls -l'
alias llh='ls -al'
alias ld='ll | grep ^d'
alias lf='ll | grep ^-'
alias ..='cd ..'
alias re='source ~/.bash_profile'
alias ea='vim ~/dotfiles/bash/aliases && re'
alias cls='clear'
alias vim='vim -N'
alias vi='vim -N'
alias e='exit'
alias refreship='ipconfig //release && ipconfig //renew'

function take {
mkdir -p $1 
cd $1
}
